<script>


 $(document).ready(function(){
  
      $('#uploadcompliment').click(function(){
        
             jQuery.ajax({
               url:"helper/functions.php",
               type:"POST",
                data: {
                    getnewrequest:true,
                    username : $('#username').val(),
                    instagramid : $('#instagramid').val(),
                    compliment : $('#compliment').val(),
                     
                   },
                 success:function(data){  
                     $('#result').html(data);
                     $('#username').val('');
                     $('#instagramid').val('');
                     $('#compliment').val('');
                     }  
             });

    });
   
 });

</script>


	
