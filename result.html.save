<script>



$(".carousel-caption").dblclick(function () {
            var val = $("#gradient_background").attr('gradient-id');

            switch (val) {
                case "1":
                    $("#gradient_background").css("background-image", "linear-gradient(#FC5C7D,#6A82FB)");
                    $(" #gradient_text").css('color', '#6A82FB ')
                    $("#gradient_background").attr('gradient-id', '2');

                    break;
                case "2":
                    $("#gradient_background").css("background", "#333");
                    $(" #gradient_text").css('color', '#333')
                    $("#gradient_background").attr('gradient-id', '3');

                    break;
                case "3":
                    $("#gradient_background").css("background-image", "linear-gradient( #CAC531,#F3F9A7)");
                    $(" #gradient_text").css('color', '#CAC531')
                    $("#gradient_background").attr('gradient-id', '4');

                    break;

                case "4":
                    $("#gradient_background").css("background-image", "linear-gradient( #e1eec3,#f05053)");
                    $(" #gradient_text").css('color', '#f05053')
                    $("#gradient_background").attr('gradient-id', '5');
                    break;
                case "5":

                    $("#gradient_background").css("background-image", "linear-gradient( #ff9966,#ff5e62)");
                    $(" #gradient_text").css('color', '#ff5e62')
                    $("#gradient_background").attr('gradient-id', '1');
                    break;
                default:
                    $(" #gradient_text").css('color', 'rgb(34, 119, 96);')
                    $("#gradient_background").css("background-image",
                        "linear-gradient(120deg, rgba(218, 241, 235, 0.705), rgba(15, 235, 176, 0.712));");
            }

        });



    function off() {
        var val = $(".carousel-control-prev").attr('state');
        if (val == 'show') {
            $(".carousel-control-prev").css("display", "none");
            $(".carousel-control-next").css("display", "none");
            $(".carousel-control-prev").attr('state', 'hide');
        } else {
            $(".carousel-control-prev").css("display", "");
            $(".carousel-control-next").css("display", "");
            $(".carousel-control-prev").attr('state', 'show');
        }

    }

    $(".carousel-control-prev").click(function () {
        $(".carousel-control-prev").attr('state', 'show');
    });
    $(".carousel-control-next").click(function () {
        $(".carousel-control-prev").attr('state', 'show');
    });

    function initialise_complement(sex ) {

            
var user = $('#username').val();
jQuery.ajax({
    url: "helper/functions.php",
    type: "GET",
    data: 'getcomplement=true&user=' + user+'&sex='+sex,
    success: function (data) {
        $('.print').html(data);

    }
});

}

    function getrandompicture() {
            var x = Math.floor((Math.random() * 127) + 1);
            $(".back").attr('src', 'bg/kashmir-' + x + '.jpg');
        }
        $(document).ready(function () {
          

            $('#modalLoginAvatar').modal({
                    backdrop: 'static',
                    keyboard: false
                },
                'show'

            )
            getrandompicture();
        });




        $(document).on('click', '#showcompliment', function () {
            var name = $("#input_username").val();
            if(name != "")
            {
                $('#modalLoginAvatar').modal('hide');
                $('#main-section').css('display', 'block');
               
                var sex = $('#sex').val();
                $("#input_username").val('');
                $(".username").html(name);
                initialise_complement(sex);

            }
           



        });
</script>


